	Doi = {10.1103/PhysRevD.70.083511},
	Eid = {083511},
	Eprint = {astro-ph/0310080},
	Journal = {\prd},
	Keywords = {Cosmology, Multivariate analysis, Computational techniques, simulations, Background radiations},
	Month = oct,
	Number = 8,
	Pages = {083511},
	Title = {{Global, exact cosmic microwave background data analysis using Gibbs sampling}},
	Volume = 70,
	Year = 2004,
